{
  "name": "ai-development-template",
  "version": "0.1.0",
  "private": true,
  "description": "Template for AI-powered service development with HeroUI and Next.js",
  "keywords": ["next.js", "react", "typescript", "heroui", "tailwindcss", "firebase", "ai", "template"],
  "author": "Development Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-development-template.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ai-development-template/issues"
  },
  "homepage": "https://github.com/your-org/ai-development-template#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"*.{ts,tsx,js,jsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx,js,jsx}\" \"*.{ts,tsx,js,jsx}\"",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --quiet",
    "tw": "tailwindcss",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run security:audit && npm outdated",
    "security:update": "npm update && npm run security:audit",
    "security:scan": "node scripts/security-check.js",
    "env:check": "node -e \"if(!process.env.NEXT_PUBLIC_FIREBASE_API_KEY) console.warn('Warning: NEXT_PUBLIC_FIREBASE_API_KEY not set');\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroui/react": "^2.7.11",
    "@heroui/theme": "^2.4.17",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "storybook": "^8.6.14"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@figma/plugin-typings": "^1.113.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/theming": "^8.6.14",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "8.57.1",
    "eslint-config-next": "^15.3.5",
    "playwright": "^1.53.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.1.6"
  }
}
